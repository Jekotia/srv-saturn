#! /bin/bash
conv () {
	if [[ "$1" == "" ]] ; then
		echo "NOPE"
		exit 1
	elif [[ "$1" == "A" || "$1" == "a" ]] ; then
		num=0
	elif [[ "$1" == "B" || "$1" == "b" ]] ; then
		num=0
	elif [[ "$1" == "C" || "$1" == "c" ]] ; then
		num=1
	elif [[ "$1" == "D" || "$1" == "d" ]] ; then
		num=1
	elif [[ "$1" == "E" || "$1" == "e" ]] ; then
		num=1
	elif [[ "$1" == "F" || "$1" == "f" ]] ; then
		num=2
	elif [[ "$1" == "G" || "$1" == "g" ]] ; then
		num=2
	elif [[ "$1" == "H" || "$1" == "h" ]] ; then
		num=3
	elif [[ "$1" == "I" || "$1" == "i" ]] ; then
		num=3
	elif [[ "$1" == "J" || "$1" == "j" ]] ; then
		num=3
	elif [[ "$1" == "K" || "$1" == "k" ]] ; then
		num=4
	elif [[ "$1" == "L" || "$1" == "l" ]] ; then
		num=4
	elif [[ "$1" == "M" || "$1" == "m" ]] ; then
		num=5
	elif [[ "$1" == "N" || "$1" == "n" ]] ; then
		num=5
	elif [[ "$1" == "O" || "$1" == "o" ]] ; then
		num=5
	elif [[ "$1" == "P" || "$1" == "p" ]] ; then
		num=6
	elif [[ "$1" == "Q" || "$1" == "q" ]] ; then
		num=6
	elif [[ "$1" == "R" || "$1" == "r" ]] ; then
		num=6
	elif [[ "$1" == "S" || "$1" == "s" ]] ; then
		num=7
	elif [[ "$1" == "T" || "$1" == "t" ]] ; then
		num=7
	elif [[ "$1" == "U" || "$1" == "u" ]] ; then
		num=8
	elif [[ "$1" == "V" || "$1" == "v" ]] ; then
		num=8
	elif [[ "$1" == "W" || "$1" == "w" ]] ; then
		num=8
	elif [[ "$1" == "X" || "$1" == "x" ]] ; then
		num=9
	elif [[ "$1" == "Y" || "$1" == "y" ]] ; then
		num=9
	elif [[ "$1" == "Z" || "$1" == "z" ]] ; then
		num=9
	fi

	echo $num
}

c1=${1:0:1}
c2=${1:1:1}
c3=${1:2:1}

echo $(conv $c1)$(conv $c2)$(conv $c3)
